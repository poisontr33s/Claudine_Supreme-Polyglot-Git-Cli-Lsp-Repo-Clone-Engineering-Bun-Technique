import { useState } from 'react';

function App() {
  const [count, setCount] = useState(0);

  return (
    <div className="app">
      <h1>{{projectName}}</h1>
      {{#if description}}
      <p>{{description}}</p>
      {{/if}}
      <div className="card">
        <button onClick={() => setCount((count) => count + 1)}>
          count is {count}
        </button>
      </div>
      <p className="footer">
        ðŸ”¥ðŸ’‹ Generated by Claudine CLI
      </p>
    </div>
  );
}

export default App;
