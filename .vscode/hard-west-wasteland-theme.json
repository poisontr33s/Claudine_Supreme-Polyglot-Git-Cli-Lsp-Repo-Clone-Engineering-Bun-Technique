      ],
      "settings": {
        "foreground": "#8B2A2A"
      }
    },
    {
      "name": "Diff Changed",
      "scope": [
        "markup.changed"
      ],
      "settings": {
        "foreground": "#D4915C"
      }
    },
    {
      "name": "JSON Key",
      "scope": [
        "support.type.property-name.json",
